    "window.menuBarVisibility": "toggle",
    "editor.fontSize": 13,
    "customizeUI.statusBarPosition": "under-panel",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": "#6272a4",
    "vim.statusBarColors.insert": "#bd93f9",
    "vim.statusBarColors.commandlineinprogress": "#44475a",
    "vim.statusBarColors.searchinprogressmode": "#44475a",
    "mdb.confirmRunAll": false,
    "terminal.integrated.minimumContrastRatio": 1,
